{
    "compilerOptions": {
      "importsNotUsedAsValues": "preserve",
      "experimentalDecorators": true,
      "target": "es2018",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": [
        "ESNext",
        "ESNext.AsyncIterable",
        "DOM"
    ],
      "esModuleInterop": true,
      "allowJs": true,
      "sourceMap": true,
      "strict": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "~/*": [
          "./*"
        ],
        "@/*": [
          "./*"
        ]
      },
      "types": [
        "@types/node",
        "@nuxt/types",
        "vuetify"
      ],
      "typeRoots": [
        "./node_modules/@types",
        "./node_modules/vuetify/types"
      ]
    },
    "exclude": [
      "node_modules"
    ]
  }